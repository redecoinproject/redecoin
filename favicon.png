<script>
  var lastBlockHeight = 0;
  function fetchBlocks() {
    fetch('https://pool2.redecoin.eu/api2/blocks')
      .then(response => response.json())
      .then(data => {
        var blocks = data.candidates;
        var latestBlock = blocks[0];

        if (latestBlock.height > lastBlockHeight) {
          showIziToast(latestBlock.height, latestBlock.worker); // przekazanie wartości worker do funkcji showIziToast()
          lastBlockHeight = latestBlock.height;
        }
      })
      .catch(error => {
        console.error('Błąd żądania API:', error);
      });
  }
  setInterval(fetchBlocks, 5000);

  function showIziToast(blockHeight, worker) {
    iziToast.info({
    title: 'New Block Found!',
    message: 'Block Height: ' + blockHeight + ' | Worker: ' + worker,
    timeout: 5000,
    position: 'topRight',
    progressBar: true
    });
  }
</script>


